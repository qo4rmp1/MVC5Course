
@{
    ViewBag.Title = "";
}

@model int[]

@{
    for (int i = 0; i < Model.Length; i++)
    {
        @Model[i]
    }
}

@{
    for (int i = 0; i < Model.Length; i++)
    {
        @(Model[i] + " ");
    }
}

<ul>
@{
    foreach (var i in Model)
    {
        <li>@i</li>
    }
  }
</ul>
